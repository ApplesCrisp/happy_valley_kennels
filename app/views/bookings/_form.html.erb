
<%= simple_form_for(@booking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pet_id %>
    <%= f.input :owner_id %>
    <%= f.input :booking_start_date %>
    <%= f.input :booking_end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
