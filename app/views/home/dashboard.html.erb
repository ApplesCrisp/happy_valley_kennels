<div id="vertical_strip"></div>
<div id="dashboard_wrapper">
  <div class="row">
    <div class="col-lg-2 profile center_content">
      <%= link_to edit_user_registration_path, class: "update_profile" do %>
        <%= image_tag "empty_profile.png", class: "simple_logo rounded-circle img-thumbnail" %>
        <p class="update_profile_msg">Update Profile</p>
